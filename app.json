{
  "expo": {
    "name": "WealthWise-Personal-Finance-Mobile",
    "slug": "WealthWise-Personal-Finance-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/images/WealthWise-Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "../assets/images/WealthWise-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "wealthwise"
  }
}
